/* pages/me/cardList/cardList.wxss */
page{
  background: #f5f5f5;
}
.cardLi{
  background: #fff;
  font-size: 30rpx;
  color: #666;
  padding: 20rpx;
  box-sizing: border-box;
}
.addCard{
  width: 60%;
  background: #44D242;
  margin-top: 40rpx;
  color: #fff;
  line-height: 80rpx;
  font-size: 30rpx;
}
.mask{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.6);
  display: flex;
  justify-content: center;
  align-items: center;
}
.maskMain{
  width: 80%;
  background: #fff;
  padding: 30rpx 40rpx;
}
.maskTitle{
  text-align: center;
  font-size: 30rpx;
}
.maskInput{
  width: 80%;
  
}
.maskMain button{
